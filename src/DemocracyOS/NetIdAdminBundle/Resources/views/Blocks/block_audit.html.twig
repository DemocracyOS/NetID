{% if is_granted('ROLE_AUDIT') or is_granted('ROLE_SUPER_ADMIN') %}
    <table class="table table-bordered table-striped sonata-ba-list">
        <thead>
            <tr>
                <th colspan="3">{{ "Audit" | trans }}</th>
            </tr>
        </thead>

        <tbody>
            <tr>
                <td>
                    <div class="btn-group" align="center">
                        <a class="btn btn-small" href="{{ admin.generateUrl('downloadLog') }}">{{ "Download log" | trans }}</a>
                    </div>
                    <p align="center"><small>md5: {{ md5 }}</small></p>
                </td>
            </tr>
        </tbody>
    </table>
{% endif %}